        <script>
            $(document).ready(()=>{
                let path= window.location.origin;
                $('#feedback').submit((e)=>{
                    e.preventDefault();
                    let feed = $('#feedback').serialize();
                    $.ajax({
                        type: 'POST',
                        url : path+'/feedback',
                        data : {
                            data : feed
                        },
                        success : (resp)=>{
                            $('.mess').toggleClass('alert');
                            $('.mess').toggleClass('alert-success');
                            $('.mess').html(`<p>${resp.message}</p>`)
                        }
                    })
                })
            })
